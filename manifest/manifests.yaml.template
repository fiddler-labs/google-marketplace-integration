        helm.sh/chart: fiddler-1.7.8
        app.kubernetes.io/instance: fiddler
        app.kubernetes.io/managed-by: Helm
        fiddler.ai/component: xai-worker
    spec:      
      serviceAccountName: fiddler      
      schedulerName: default-scheduler      
