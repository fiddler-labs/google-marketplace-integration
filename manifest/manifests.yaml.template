            - bash
            - -c
            - |-
              if [[ -f /var/lib/fiddler/runtime/env ]]; then
                echo "Found /var/lib/fiddler/runtime/env; sourcing"
                source /var/lib/fiddler/runtime/env
              else
                echo "File not found: /var/lib/fiddler/runtime/env; proceeding anyway"
